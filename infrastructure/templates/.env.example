# AI API Keys

# OpenAI (most projects)
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4o-mini

# Anthropic (ClaudeForge)
ANTHROPIC_API_KEY=sk-ant-...
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022

# Google (MarketPulse)
GOOGLE_API_KEY=...
GOOGLE_ADK_PROJECT_ID=...

# Framework-specific
LANGCHAIN_API_KEY=...
LANGGRAPH_API_KEY=...
LLAMA_INDEX_API_KEY=...
CREWAI_API_KEY=...
HAYSTACK_API_KEY=...
AUTOGEN_API_KEY=...

# Database (TrendFactory, SupplyConsensus)
DATABASE_URL=postgresql://user:password@localhost:5432/dbname
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=ai_sdk_db
DATABASE_USER=postgres
DATABASE_PASSWORD=...

# Frontend/Backend Communication
# Next.js projects (VentureGraph, InsightStream, ClaudeForge)
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_API_BASE=/api

# Nuxt projects (TrendFactory)
NUXT_PUBLIC_API_BASE=http://localhost:8000

# Vue/React projects (OmniDesk, SupplyConsensus, MarketPulse)
VITE_API_URL=http://localhost:8000

# Deployment
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Cloud Deployment
# GCP (MarketPulse)
GCP_PROJECT_ID=your-project-id
GCP_SA_KEY={"type":"service_account",...}

# Fly.io (DevSquad, ResearchSynthesis, ClaudeForge)
FLY_API_TOKEN=...
FLY_APP_NAME=...

# AWS (PatentIQ)
AWS_ACCESS_KEY_ID=...
AWS_SECRET_ACCESS_KEY=...
AWS_REGION=us-east-1
ECR_REPOSITORY=...

# Vercel (InsightStream)
VERCEL_TOKEN=...
VERCEL_PROJECT_ID=...

# Railway (VentureGraph)
RAILWAY_TOKEN=...

# Render (OmniDesk, TrendFactory)
RENDER_API_KEY=...

# Security
SECRET_KEY=your-secret-key-here
JWT_SECRET=your-jwt-secret
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173
ALLOWED_HOSTS=localhost,127.0.0.1
